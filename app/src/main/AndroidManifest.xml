<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="oly.apps.sathv.ohsfbla"
    android:versionCode="29">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <supports-screens
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:name="oly.apps.sathv.ohsfbla.FireApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:largeHeap="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:windowSoftInputMode="adjustPan">
        <activity
            android:name="oly.apps.sathv.ohsfbla.SplashScreen"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="oly.apps.sathv.ohsfbla.LockScreen"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.FBLAHome"
            android:label="@string/title_activity_fblahome"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="com.example.sathv.ohsfbla_TARGET_FH" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="oly.apps.sathv.ohsfbla.CreateFBLAAccount"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.Details"
            android:screenOrientation="portrait" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.RatingSheet"
            android:screenOrientation="portrait" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.Instructions"
            android:screenOrientation="portrait" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.PaymentInformation"
            android:screenOrientation="portrait" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.Head"
            android:screenOrientation="portrait" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.FallDetails"
            android:screenOrientation="portrait" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.StateDetails"
            android:screenOrientation="portrait" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.SponsorDetails"
            android:screenOrientation="portrait" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.example.sathv.ohsfbla_TARGET_AS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="oly.apps.sathv.ohsfbla.PostActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />

        <service android:name="oly.apps.sathv.ohsfbla.MyFirebaseInstanceIdService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name="oly.apps.sathv.ohsfbla.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name="oly.apps.sathv.ohsfbla.Setup"
            android:screenOrientation="portrait" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.Profile"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.PostSingleActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.ANote"
            android:screenOrientation="portrait" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.PopupActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.Hello"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="oly.apps.sathv.ohsfbla.Settings" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.TabbedActivity"
            android:label="@string/title_activity_tabbed"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="oly.apps.sathv.ohsfbla.Approvals" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.ChatActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="oly.apps.sathv.ohsfbla.FullImage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="oly.apps.sathv.ohsfbla.CommentActivity" />
        <activity android:name="oly.apps.sathv.ohsfbla.OtherProfile"></activity>
    </application>

</manifest>